{
  "name": "ivoba/superleansilexplate",
  "type": "silex-starter",
  "description": "Superleansilexplate",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivo Bathke",
      "email": "mail@feednapi.net"
    }
  ],
  "require": {
    "php": ">=5.4",
    "silex/silex": "^1.3",
    "symfony/console": "^2.8",
    "symfony/finder" : "^2.8",
    "symfony/filesystem": "^2.8",
    "vlucas/phpdotenv": "^2.2",
    "twig/twig": "^1.16",
    "knplabs/console-service-provider": "^1",
    "symfony/event-dispatcher": "~2.8",
    "symfony/http-foundation": "~2.8",
    "symfony/http-kernel": "^2.8",
    "symfony/routing": "^2.8"
  },
  "require-dev": {
    "ivoba/stop": "^0.2",
    "symfony/var-dumper": "^2.8",
    "symfony/browser-kit": "^2.8",
    "symfony/css-selector": "^2.8",
    "symfony/dom-crawler": "^2.8",
    "symfony/debug": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "Superleansilexplate\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "bower install",
      "chmod 777 cache",
      "cp .env.dist .env"
    ],
    "post-update-cmd": [
      "bower install"
    ]
  },
  "suggest": {
    "ivoba/dotenv-service-provider": "Advanced Service Provider for dotenv"
  }
}

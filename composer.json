{
    "name": "ivoba/superleansilexplate",
    "type": "silex-starter",
    "description": "Superleansilexplate",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivo Bathke",
            "email": "mail@feednapi.net"
        }
    ],
    "require": {
        "php": ">=5.5",
        "silex/silex": "^1.3",
        "vlucas/phpdotenv": "^2.2",
        "twig/twig": "^1.16",
        "ivoba/console-service-provider": "^2.0",
        "symfony/finder": "^3.0",
        "symfony/filesystem": "^3.0"
    },
    "require-dev": {
        "ivoba/stop": "^0.2",
        "symfony/var-dumper": "^3.0",
        "symfony/browser-kit": "^3.0",
        "symfony/css-selector": "^3.0",
        "symfony/dom-crawler": "^3.0",
        "symfony/debug": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Superleansilexplate\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "bower install",
            "chmod 777 cache",
            "cp .env.dist .env"
        ],
        "post-update-cmd": [
            "bower install"
        ]
    },
    "suggest": {
        "ivoba/dotenv-service-provider": "Advanced Service Provider for dotenv"
    }
}
